    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}", // 
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "--image_alone_path",
                "dataset/Reann_MPSC/jyryu/lmdb/train/mpsc.lmdb.train",
                "--mask_ratio",
                "0.7",
                "--batch_size",
                "64",
                "--opt",
                "adamw",
                "--output_dir",
                "output/mpsc/temp",
                "--epochs",
                "10",
                "--warmup_steps",
                "100",
                "--max_len",
                "25",
                "--num_view",
                "2",
                "--moco_dim",
                "256",
                "--moco_mlp_dim",
                "4096",
                "--moco_m",
                "0.99",
                "--moco_m_cos",
                "--moco_t",
                "0.2",
                "--num_windows",
                "4",
                "--contrast_warmup_steps",
                "0",
                "--contrast_start_epoch",
                "0",
                "--loss_weight_pixel",
                "1.",
                "--loss_weight_contrast",
                "0.1",
                "--only_mim_on_ori_img",
                "--weight_decay",
                "0.1",
                "--opt_betas",
                "0.9",
                "0.999",
                "--model",
                "pretrain_simmim_moco_ori_vit_small_patch4_32x128",
                "--patchnet_name",
                "no_patchtrans",
                "--encoder_type",
                "vit"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "4",
                "OMP_NUM_THREADS": "1"
            }
        },
        {
            "name": "Python Debugger: run_mae_pretraining_moco.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_mae_pretraining_moco.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "--image_alone_path",
                "dataset/Reann_MPSC/jyryu/lmdb/train/mpsc.lmdb.train",
                "--mask_ratio",
                "0.7",
                "--batch_size",
                "64",
                "--opt",
                "adamw",
                "--output_dir",
                "output/mpsc/temp",
                "--epochs",
                "10",
                "--warmup_steps",
                "100",
                "--max_len",
                "25",
                "--num_view",
                "2",
                "--moco_dim",
                "256",
                "--moco_mlp_dim",
                "4096",
                "--moco_m",
                "0.99",
                "--moco_m_cos",
                "--moco_t",
                "0.2",
                "--num_windows",
                "4",
                "--contrast_warmup_steps",
                "0",
                "--contrast_start_epoch",
                "0",
                "--loss_weight_pixel",
                "1.",
                "--loss_weight_contrast",
                "0.1",
                "--only_mim_on_ori_img",
                "--weight_decay",
                "0.1",
                "--opt_betas",
                "0.9",
                "0.999",
                "--model",
                "pretrain_simmim_moco_ori_vit_small_patch4_32x128",
                "--patchnet_name",
                "no_patchtrans",
                "--encoder_type",
                "vit"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "4",
                "OMP_NUM_THREADS": "1"
            }
        },
        {
            "name": "Python Debugger: Finetuning SimMIM",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "program": "run_class_finetuning.py",
            "env": {
                "CUDA_VISIBLE_DEVICES": "2,3,4,5",
                "OMP_NUM_THREADS": "1",
                "DATA_PATH": "dataset/Reann_MPSC/jyryu/lmdb/train/mpsc.lmdb.train", // 환경 변수 설정
                "MODEL_PATH_PRE": "checkpoint-9.pth",
                "OUTPUT_DIR": "output/mpsc/temp"
            },
            "args": [
                "--nproc_per_node=1",
                "--master_port=10040",
                "--model",
                "simmim_vit_small_patch4_32x128",
                "--data_path",
                "${env:DATA_PATH}",
                "--eval_data_path",
                "${env:DATA_PATH}",
                "--finetune",
                "${env:MODEL_PATH_PRE}",
                "--output_dir",
                "${env:OUTPUT_DIR}",
                "--batch_size",
                "128",
                "--opt",
                "adamw",
                "--opt_betas",
                "0.9",
                "0.999",
                "--weight_decay",
                "0.05",
                "--data_set",
                "image_lmdb",
                "--nb_classes",
                "97",
                "--smoothing",
                "0.",
                "--max_len",
                "25",
                "--epochs",
                "90",
                "--warmup_epochs",
                "1",
                "--drop",
                "0.1",
                "--attn_drop_rate",
                "0.1",
                "--drop_path",
                "0.1",
                "--lr",
                "1e-4",
                "--num_samples",
                "1",
                "--fixed_encoder_layers",
                "0",
                "--decoder_name",
                "tf_decoder",
                "--use_abi_aug",
                "--num_view",
                "2",
                "--disable_eval_during_finetuning"
            ],
            "console": "integratedTerminal"
        },
    ]
}